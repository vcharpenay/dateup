doctype html
html
  head
    meta(charset='utf-8')
    link(rel='stylesheet', href='/css/style-view.css')
    link(rel='stylesheet', href='/css/card-stack.css')
    link(rel='stylesheet', href='/css/animate.css')
    script(src='/js/app-view.js')
    script(src='/js/swing.min.js')
    script(src='/js/classList.js')
    script(src='/js/card-stack.js')
  body
    #viewport
      ul.stack 
        div.bottom-stack That's it for today ! 
        each i in user.dateups
            li.clubs= i.partner.name
                img#dateup-profilepic(src= user.profilepic) 

    img#profile-image.animated.bounceInUp(src= user.profilepic) 
    #userprofile.animated.bounceInUp= user.name 

    
